{
  "name": "swagger-jsdoc-express",
  "version": "4.0.0",
  "description": "Parses JSDoc comments from files and strings and set ups Swagger UI from it, to be used with Express framework.",
  "main": "lib/index.js",
  "scripts": {
    "build": "(rm -r ./lib || true) && tsc & tsc -d",
    "deploy": "npm install && npm run build && npm publish",
    "sandbox": "npm run build && node ./sandbox.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "doc": "(rm -r ./doc || true) && (typedoc --out ./doc ./src)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egodigital/swagger-jsdoc-express"
  },
  "keywords": [
    "express",
    "jsdoc",
    "swagger",
    "ui",
    "parser",
    "generator",
    "api",
    "documentation",
    "http",
    "server",
    "rest",
    "restful",
    "router",
    "app"
  ],
  "author": "e.GO Digital GmbH, Aachen, Germany",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/egodigital/swagger-jsdoc-express/issues"
  },
  "homepage": "https://github.com/egodigital/swagger-jsdoc-express#readme",
  "devDependencies": {
    "@egodigital/tsconfig": "^1.3.0",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.8",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/doctrine": "^0.0.3",
    "@types/express": "4.17.1",
    "@types/fs-extra": "^8.0.1",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.17.13",
    "@types/swagger-ui-express": "^4.1.1",
    "deepmerge": "^4.2.2",
    "doctrine": "^3.0.0",
    "express": "^4.17.1",
    "fast-glob": "^3.1.1",
    "fs-extra": "^8.1.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "swagger-ui-express": "^4.1.3"
  }
}
